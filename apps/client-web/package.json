{
  "name": "client-web",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "patch-esbuild": "node -e \"try { require.resolve('remix-esbuild-override'); process.exit(1); } catch { process.exit(0); }\" || yarn run remix-esbuild-override",
    "postinstall": "yarn run patch-esbuild",
    "prebuild": "yarn run patch-esbuild",
    "build": "yarn run build:css && remix build",
    "dev": "remix dev",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "deploy": "cd ../../ && flyctl deploy --dockerfile ./apps/client-web/Dockerfile --config ./apps/client-web/fly.toml",
    "test": "cross-env NODE_ENV=test yarn dev"
  },
  "dependencies": {
