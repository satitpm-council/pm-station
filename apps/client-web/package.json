    "@remix-run/react": "^1.19.1",
    "@remix-run/vercel": "^1.19.1",
    "@sentry/remix": "^7.13.0",
    "@station/client": "workspace:*",
    "@station/server": "workspace:*",
    "@station/shared": "workspace:*",
    "@upstash/redis": "1.20.0",
    "@vercel/node": "^2.10.3",
    "algoliasearch": "^4.14.2",
    "axios": "^0.27.2",
    "axios-retry": "^3.3.1",
